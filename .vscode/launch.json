{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Linux Debug Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/DvmEngine",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build DvmEngine"
    },
    {
      "name": "Windows Debug Launch",
